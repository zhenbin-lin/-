    ohos:height="match_content"
    ohos:width="match_content"
    ohos:orientation="vertical"
    ohos:padding="20vp">

    <!--Player-->
    <DependentLayout
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:orientation="vertical">

        <Text
            ohos:id="$+id:player_title"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:align_parent_start="true"
            ohos:align_parent_top="true"
            ohos:text="Player"
            ohos:text_size="15fp"/>

        <Text
            ohos:id="$+id:player"
            ohos:height="match_content"
            ohos:width="55vp"
            ohos:below="$+id:player_title"
            ohos:text="Player"
            ohos:text_size="10fp"/>

        <Text
            ohos:id="$+id:player_value"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:below="$+id:player_title"
            ohos:right_of="$+id:player"
            ohos:start_margin="50vp"
            ohos:text="N/A"
            ohos:text_size="10fp"/>
    </DependentLayout>

